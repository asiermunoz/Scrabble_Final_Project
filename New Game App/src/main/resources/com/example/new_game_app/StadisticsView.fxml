                <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="814.0" style="-fx-background-color: #ed6e5a;" text="Estadisticas" textFill="#9e0000">
                    <font>
                        <Font name="Cooper Black" size="28.0" />
                    </font>
                </Label>
            </children>
        </HBox>
      <HBox alignment="CENTER" prefHeight="286.0" prefWidth="790.0">
         <children>
             <BarChart fx:id="barChart" prefHeight="286.0" prefWidth="567.0">
                 <xAxis>
                     <NumberAxis side="BOTTOM" />
                 </xAxis>
                 <yAxis>
                     <CategoryAxis side="LEFT" />
                 </yAxis>
             </BarChart>
         </children>
      </HBox>
    </children>
