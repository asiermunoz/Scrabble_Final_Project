<VBox prefHeight="330.0" prefWidth="789.0" style="-fx-background-color: #f3dbc3;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.new_game_app.StadisticsController">
    <children>
        <HBox alignment="TOP_CENTER" prefHeight="42.0" prefWidth="600.0" style="-fx-background-color: #48b55b;">
